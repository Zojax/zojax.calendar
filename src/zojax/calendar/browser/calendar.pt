<tal:block i18n:domain="zojax.calendar" tal:define="hasEvents view/hasEvents">
  <h1 i18n:translate="">Calendar</h1>

  <div tal:condition="not:hasEvents" i18n:translate="">
    No events have been created.
  </div>

  <div tal:condition="hasEvents">


    <script type="text/javascript">
$(document).ready(function() {

$.ajax({
    type: "POST",
    url: "http://localhost:8080/++skin++JSONRPC.authentication/QuickIntranet",
    data: "{'method':'listCalendar', 'params': {'showdate': '07/10/01 10:00', 'viewtype': 'month'}}",
    contentType: "application/json; charset=utf-8",
    dataType: "json",
    success: function(msg) {
      console.log(msg);
    }
  });

});
    </script>


    <tal:block repeat="event view/events">
      <a href="#" tal:attributes="href event/@@absolute_url" tal:content="event/title"></a> <br />
    </tal:block>
  </div>

</tal:block>
